FROM fedora:latest  
  
RUN dnf upgrade -y python3  
RUN pip3 install --upgrade pip  
RUN python3 --version  
RUN pip3 install \  
fonttools \  
jsmin \  
markdown \  
msgpack-python \  
numpy \  
pareto \  
pillow \  
pyinstaller \  
pytest \  
pyyaml \  
pyzmq \  
requests \  
tornado \  
wheel \  
xlrd  
  

